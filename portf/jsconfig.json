{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@bits-projects": [
        "bits-projects/index.js"
      ],
      "@bits-general": [
        "bits-general/index.js"
      ],
      "@pages/general": [
        "pages/general/index.js"
      ],
      "@pages-general/*": [
        "pages-general/*"
      ],
      "@pages-projects/*": [
        "pages-projects/*"
      ],
      "@figures/*": [
        "figures/*"
      ],
      "@constants/*": [
        "constants/*"
      ],
      "@styles/*": [
        "styles/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}